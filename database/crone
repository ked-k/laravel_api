# Step 1: Open the nano editor and create the script
nano ~/curl_every_10_seconds.sh

# Inside nano, paste the script content:
#!/bin/bash

while true; do
    /usr/bin/curl http://192.168.88.2/eafya-patient
    sleep 10  # Wait for 10 seconds before the next execution
done

# Save and close nano (press CTRL + X, then Y to confirm and ENTER to save)

# Step 2: Make the script executable
chmod +x ~/curl_every_10_seconds.sh

# Step 3: Run the script in the background using nohup
nohup ~/curl_every_10_seconds.sh &

# Step 4: Verify that the script is running
ps aux | grep curl_every_10_seconds.sh


